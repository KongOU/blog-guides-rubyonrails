<!DOCTYPE html>
<html lang="en">
<head>
  <%= render 'layouts/meta' %>
  <title>Hello Rails</title>
  <%= render 'layouts/shim' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div class="container">
    <%= render 'layouts/flash' %>
    <%= yield %>
  </div>
</body>
</html>
